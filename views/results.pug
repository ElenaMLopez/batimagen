doctype html
html(lang='en')
  head
    include includes/head.pug
  body
    include includes/logo.pug
    h3 Metadata:
    ul
      for field in data.metadata.data
        each key, value in field
            li #{value}: #{key}
      else
        li sorry, no info!
    if data.metadata.error
      strong Error?: #{error}
    h3 Virus:
    pre 
      code #{JSON.stringify(data.virusData, null, 2)}
    h3 Vision:
    pre 
      code #{JSON.stringify(data.vision, null, 2)}
    include includes/scripts.pug
